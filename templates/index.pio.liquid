{%- comment -%}Please DO NOT edit this file{%- endcomment -%}
        {%- layout none -%}
        {%- assign pioParamKey = 'q' -%}
        {%- assign pioParamValue = '' -%}
        {%- capture contentForQuerystring -%}{{ content_for_header }}{%- endcapture -%}
        {%- assign pioPageUrl = contentForQuerystring | split:'"pageurl":"' | last | split:'"' | first | split:'.myshopify.com' | last |
           replace:'\/','/' |
           replace:'%20',' ' |
           replace:'\u0026','&'
        -%}
        {%- assign pageQuerystring = pioPageUrl | split:'?' | last -%}
        {%- assign pioParts = pageQuerystring | split:'&' -%}
        {%- for pioPart in pioParts -%}
          {%- assign pioKeyAndValue = pioPart | split:'=' -%}
          {%- if pioKeyAndValue[0] == pioParamKey -%}
            {%- assign pioParamValue = pioKeyAndValue[1] | strip -%}
          {%- endif -%}
        {%- endfor -%}
        {%- assign namespace = 'td_' | append: pioParamValue -%}
        {
            "metafields": {
            "shop": {{ shop.permanent_domain | json }},
            "settings": {{ shop.metafields[namespace].app_settings }},
            "moneyFormat": {{ shop.money_format | json }},
            "namespace": "{{namespace}}"
          }
        }